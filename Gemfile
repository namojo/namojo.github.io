source "https://rubygems.org"

# 이것이 핵심입니다 - GitHub Pages 젬을 사용합니다
# gem "github-pages", group: :jekyll_plugins

gem 'github-pages', '~> 228', group: :jekyll_plugins
gem 'jekyll', '~> 3.9.3'  # GitHub Pages와 호환되는 버전

# 필요한 플러그인들
group :jekyll_plugins do
  gem "jekyll-sitemap"
  gem "jekyll-paginate"
  gem "jemoji"
end

